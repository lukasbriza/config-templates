{
  "name": "@lukasbriza/config-templates",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "exports": {
    "./ts": "./ts-config/base.json",
    "./ts/app":"./ts-config/app.json",
    "./ts/cjs-library":"./ts-config/cjs-library.json",
    "./ts/library":"./ts-config/library.json",
    "./ts/nestjs":"./ts-config/nestjs.json",
    "./ts/nextjs":"./ts-config/nextjs.json",

    "./prettier": "./prettier-config/dist/index.js",
    
    "./eslint": "./eslint-config/dist/base.js",
    "./eslint/cjs":"./eslint-config/dist/cjs.js",
    "./eslint/nestjs":"./eslint-config/dist/nestjs.js",
    "./eslint/nextjs":"./eslint-config/dist/nextjs.js",
    "./eslint/react":"./eslint-config/dist/react.js",
    "./eslint/storybook":"./eslint-config/dist/storybook.js"
  },
  "files": [
    "prettier-config/dist",
    "eslint-config/dist",
    "ts-config/app.json",
    "ts-config/cjs-library.json",
    "ts-config/library.json",
    "ts-config/nestjs.json",
    "ts-config/nextjs.json",
    "ts-config/base.json"
  ],

  "scripts": {
    "prepare": "node scripts/husky-prepare.js",
    "prepare:husky": "husky",
    "prebuild:prettier": "rimraf prettier-config/dist prettier-config/tsconfig.build.tsbuildinfo",
    "build:prettier": "tsc --build prettier-config/tsconfig.build.json",
    "prebuild:eslint": "rimraf eslint-config/dist eslint-config/tsconfig.build.tsbuildinfo",
    "build:eslint": "tsc --build eslint-config/tsconfig.build.json",
    "build": "pnpm build:prettier && pnpm build:eslint"
  },

  "devDependencies": {
    "rimraf": "5.0.7",
    "eslint": "8.57.0",
    "prettier": "3.3.2",
    "prettier-plugin-packagejson": "^2.5.0",
    "typescript": "5.5.3",
    "husky": "9.0.11",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@next/eslint-plugin-next": "14.2.4",
    "@types/eslint": "8.56.10",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-turbo": "2.0.6",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-formatjs": "4.13.3",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest-dom": "5.4.0",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.34.3",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-storybook": "0.8.0",
    "eslint-plugin-testing-library": "6.2.2",
    "eslint-plugin-unicorn": "54.0.0",
    "eslint-plugin-unused-imports": "3.2.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^2.0.6",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-formatjs": "^4.13.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "eslint-plugin-unicorn": "^54.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },

  "packageManager": "pnpm@9.4.0"
}
